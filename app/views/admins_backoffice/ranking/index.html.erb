<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header"><%= t('message.listing', model: @users.model_name.human(count: 2)) %></h1>
  </div>
    <!-- /.col-lg-12 -->
</div>

<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>#</th>
                <th><%= translate_attribute(@users, :email) %></th>
                <th>Respostas corretas</th>
                <th>Respostas incorretas</th>
            </tr>
        </thead>
        <tbody>
            <% @users.each do |user| %>
            <tr>
                <td> <%= user.id %> </td>
                <td> <%= user.email %> </td>
                <td> <%= user.user_statistic.right_questions %></td>
                <td> <%= user.user_statistic.wrong_questions%></td>
                </td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>
